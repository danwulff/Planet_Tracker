<div class = "user center">
  <h3 class="apply-font">Login</h3>
  <br>
  <form action="/usersLogin" method="post">
    <label for="userNameLogin" class="font-shadow">Input Your UserName:</label>
    <input type="text" name="userNameLogin" class="font-black">
    <br>
    <br>
    <label for="passwordLogin" class="font-shadow">Input Your Password:</label>
    <input type="text" name="passwordLogin" class="font-black">
    <br>
    <br>
    <button type="submit" name="loginUser" class="btn btn-default font-black">Login</button>
  </form>
  <br>
  <a href="/users" class="btn btn-default">Make a Profile</a>
</div>
